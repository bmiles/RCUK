            <xs:element name="intellectualProperty" type="gtr-pout:intellectualProperty" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="policyInfluence" type="gtr-pout:policyInfluence" minOccurs="0" maxOccurs="unbounded"/>
